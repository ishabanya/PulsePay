{
  "name": "payment-backend",
  "version": "1.0.0",
  "description": "Payment processing backend with Node.js, Express, Firebase, and Stripe",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node --project tsconfig.json src/server.ts",
